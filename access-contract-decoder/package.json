{
  "name": "@san-dev/access-contract-decoder",
  "version": "1.0.0",
  "description": "Auto-generated TypeScript types and utilities for AccessContract smart contract",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "web3",
    "ethereum",
    "smart-contracts",
    "typescript",
    "solidity",
    "abi",
    "event-decoding"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/web3-template.git",
    "directory": "access-contract-decoder"
  },
  "homepage": "https://github.com/your-username/web3-template#readme",
  "bugs": {
    "url": "https://github.com/your-username/web3-template/issues"
  },
  "scripts": {
    "prepack": "npm run build && npm run test",
    "update:abi": "ts-node scripts/update-abi.ts",
    "generate:constants": "ts-node scripts/generate-constants.ts",
    "generate:types": "ts-node scripts/generate-types.ts",
    "generate:decoders": "ts-node scripts/generate-decoders.ts",
    "generate:index": "ts-node scripts/generate-index.ts",
    "generate:docs": "ts-node scripts/generate-docs.ts",
    "generate:all": "npm run generate:constants && npm run generate:types && npm run generate:decoders && npm run generate:index",
    "build": "npm run update:abi && npm run generate:all && npm run generate:docs && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.8.0",
    "dotenv": "^17.2.1"
  }
}
